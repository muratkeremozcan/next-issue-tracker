{
  "compilerOptions": {
    "lib": ["esnext", "dom"],
    "types": ["cypress", "node", "@bahmutov/cy-api", "cypress-map"],
    "baseUrl": "./",
    "paths": {
      "@support/*": ["support/*"],
      "@fixtures/*": ["fixtures/*"],
      "@/*": ["../*"]
    },
    "allowJs": true,
    "resolveJsonModule": true,
    "esModuleInterop": true
  },
  "include": ["**/*.ts*", "../cypress.d.ts"],
  "extends": "../tsconfig.json"
}
